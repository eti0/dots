  #ownaction = "{action %p$*}";

  #ownaction_target = "{action_core $0}%Y:%c$1%n ";

  #pvtaction = "%y (*) $*%n ";
  #pvtaction_query = "{action $* }";

  #pubaction = "{action $*}";

  whois = "%K    │  %n$1-";

  ownnotice = "%NNote n = %Mnew%n $0 ($1-) ";
  notice = "%M$*%n ";
  pubnotice_channel = " %N($*)";
  pvtnotice_host = " %N($*)";
  servernotice = " %N($*)";

  ownctcp = "%N$0 (%K$1-%N) ";
  ctcp = "%N$*%n";

  wallop = "%K$*%n: ";
  wallop_nick = "%n$*";
  wallop_action = "%K * $*%n ";

  #netsplit = "%K$*%n";
  #netjoin = "%K$*%n";

  names_users = "%K┌──┐─%w $1     %n\012%W└%n─%K─│───%n──────%W──────%c──────%C──────%c──────%C────────────────%K─────────── %K─ ─  %n── %K─";
  names_nick = "%K(%W$2$0%n$1%K)%n ";
  names_nick_op = "{names_nick $* %W}";
  names_nick_halfop = "{names_nick $* %W}";
  names_nick_voice = "{names_nick $* %W}";
  names_prefix = "%K   │ %n$1";
  names_channel = "%W$*%n";

  dcc = "%g$*%n";
  dccfile = "%_$*%_";

  dccownmsg = "%g /* $0 ($1-) */";
  dccownnick = "$*%n";
  dccownquerynick = "$*%n";
  dccownaction = "{action $*}";
  dccownaction_target = "{action $*}";

  dccmsg = "%g/* $1- ($0) */";
  dccquerynick = "%g$*%n";
  dccaction = "{action $*}";

  sb_background = "%N%N";

  sb_prompt_bg = "%n";

  sbstart = "%K┌─%n┐ ";
  sbend = "";

  topicsbstart = "%BChannel: %b\"$*";
  topicsbend = "$*%b\"";

  prompt = "%K└%n─%W──%n─ %n";
